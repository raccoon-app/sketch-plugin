function openURL(url){
  var nsurl = NSURL.URLWithString(url + '?ref=smp');
  NSWorkspace.sharedWorkspace().openURL(nsurl)
}

function linkDonate(){
  openURL('https://github.com/raccoon-app');
}

function linkDocumentation(){
  openURL('https://github.com/raccoon-app/sketch-plugin/');
}

function linkFeedback(){
  openURL('https://github.com/raccoon-app/sketch-plugin/issues');
}
